<p id="notice"><%= notice %></p>

<h1>Installations</h1>

<table>
  <thead>
    <tr>
      <th>Unit</th>
      <th>Room name</th>
      <th>Product</th>
      <th>Installer</th>
      <th>Install date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @installations.each do |installation| %>
      <tr>
        <td><%= installation.unit.unit %></td>
        <td><%= installation.room_name.name %></td>
        <td><%= installation.product.color %></td>
        <td><%= installation.installer.name %></td>
        <td><%= installation.install_date %></td>
        <td><%= link_to 'Show', installation %></td>
        <td><%= link_to 'Edit', edit_installation_path(installation) %></td>
        <td><%= link_to 'Destroy', installation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Installation', new_installation_path %>
